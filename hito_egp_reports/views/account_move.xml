<odoo>
  <record id="view_move_form_inherit_dolar_hoy" model="ir.ui.view">
    <field name="name">account.move.form.inherit.dolar.hoy</field>
    <field name="model">account.move</field>
    <field name="inherit_id" ref="account.view_move_form"/>
    <field name="arch" type="xml">
      <xpath expr="//field[@name='invoice_date']" position="after">
        <field name="price_dolar" invisible="1"/>
      </xpath>
    </field>
  </record>
</odoo>