<odoo>
  <record id="view_move_form_inherit_res_company" model="ir.ui.view">
    <field name="name">account.move.form.inherit.res.company</field>
    <field name="model">res.company</field>
    <field name="inherit_id" ref="base.view_company_form"/>
    <field name="arch" type="xml">
      <xpath expr="//field[@name='currency_id']" position="after">
          <field name="fondo_remito" widget="image"/>
      </xpath>
    </field>
  </record>
</odoo>